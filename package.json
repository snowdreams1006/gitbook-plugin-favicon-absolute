{
  "name": "gitbook-plugin-favicon-absolute",
  "version": "0.0.3",
  "description": "favicon-absolute 插件采用绝对路径设置网站 favicon 图标,相对于相对路径来说更加简单方便.",
  "main": "index.js",
  "engines": {
    "gitbook": "*"
  },
  "gitbook": {
    "properties": {
      "shortcut": {
        "type": "string",
        "required": true,
        "title": "网页图标",
        "description": "网页打开时显示图标"
      },
      "bookmark": {
        "type": "string",
        "required": true,
        "title": "收藏夹图标",
        "description": "网页被收藏时显示图标"
      },
      "appleTouch": {
        "type": "string",
        "required": false,
        "title": "Apple 设备默认触摸图标",
        "description": "Apple 设备触摸时默认显示图标"
      },
      "appleTouchMore": {
        "type": "object",
        "required": false,
        "title": "Apple 设备自定义尺寸触摸图标",
        "description": "Apple 设备触摸时更多尺寸显示图标"
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/snowdreams1006/gitbook-plugin-favicon-absolute.git"
  },
  "keywords": [
    "gitbook-plugin-favicon-absolute",
    "favicon",
    "absolute",
    "apple-touch-icon",
    "shortcut",
    "icon",
    "bookmark"
  ],
  "author": {
    "name": "snowdreams1006",
    "email": "snowdreams1006@163.com"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/snowdreams1006/gitbook-plugin-favicon-absolute/issues"
  },
  "homepage": "https://snowdreams1006.github.io/gitbook-plugin-favicon-absolute/"
}
